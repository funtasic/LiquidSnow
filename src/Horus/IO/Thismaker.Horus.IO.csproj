<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Version>3.1.1</Version>
    <Authors>Thismaker</Authors>
    <Description>Enables you to quickly secure your files and folders by providing the CryptoFile and CryptoContainer classes for dealing with files and folders respectively.</Description>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageTags>encryption encrypt-files</PackageTags>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageIcon>Thisicon-Prime.png</PackageIcon>
    <RepositoryUrl>https://github.com/ocluse/LiquidSnow</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright>Â© Ocluse. All rights reserved</Copyright>
    <Company>Ocluse</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
	<PackageOutputPath>..\..\..\build</PackageOutputPath>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Packaging" Version="6.0.0" />
    <PackageReference Include="System.Text.Json" Version="6.0.4" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Horus\Thismaker.Horus.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\assets\images\Thisicon-Prime.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
