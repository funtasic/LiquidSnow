<Application x:Class="Thismaker.Thoth.Manager.Wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Thismaker.Thoth.Manager.Wpf"
             xmlns:goro="http://schemas.thismaker.com/liquidsnow/goro"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Thismaker.Goro;component/styles.xaml"/>
                <ResourceDictionary>
                    <local:LocaleConverter x:Key="localeConverter"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
